<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.ram.hibernate.Product" table="employee" lazy="false">
<id name="productnumber" type="int">
<generator class="increment"></generator>
</id>
<property name="productname" column="productname"></property>
<property name="imgurl"></property>
<property name="details"></property>
<property name="price"></property>
</class>
</hibernate-mapping>
